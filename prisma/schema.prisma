// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Building {
  id Int @id @default(autoincrement())

  name String
  address String
  zipCode String
  contactName String
  contactPhone String

  lat Float
  lng Float

  seismicZone String
  distanceFromFaultLine Float
  soilType String
}
